@page
@model Teste.Pages.AssociadosEmpresas.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Novo Cadastro</h1>

<h4>Associados e Empresas</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="AssociadoEmpresaEntity.CD_empresa" class="control-label"></label>
                <select asp-for="AssociadoEmpresaEntity.CD_empresa" class ="form-control" asp-items="ViewBag.CD_empresa"></select>
            </div>
            <br />
            <div class="form-group">
                <label asp-for="AssociadoEmpresaEntity.CD_associado" class="control-label"></label>
                <select asp-for="AssociadoEmpresaEntity.CD_associado" class ="form-control" asp-items="ViewBag.CD_associado"></select>
            </div>
            <br />
            <div class="form-group">
                <input type="submit" value="Cadastrar" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>
<br />
<div>
    <a asp-page="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
